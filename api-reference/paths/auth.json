{
    "openapi": "3.0.1",
    "info": {
        "title": "DigitalU API",
        "description": "DigitalU API",
        "license": {
            "name": "MIT"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://squadz.ngrok.app/api"
        }
    ],
    "security": [
        {
            "Authorization": []
        }
    ],
    "paths": {
        "/upload": {
            "get": {
                "tags": ["Utility"],
                "summary": "Upload file",
                "parameters": [
                    {
                        "name": "public",
                        "in": "query",
                        "required": false,
                        "schema": { "type": "string" },
                        "example": "true"
                    },
                    {
                        "name": "orderby",
                        "in": "query",
                        "required": false,
                        "schema": { "type": "string" },
                        "example": "createdate desc"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "createdate": {
                                                "type": "string",
                                                "format": "date-time"
                                            },
                                            "id": {
                                                "type": "string"
                                            },
                                            "name": {
                                                "type": "string"
                                            },
                                            "result": {
                                                "type": "string",
                                                "format": "uri"
                                            },
                                            "state": {
                                                "type": "string",
                                                "enum": ["complete", "processing", "failed"]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
      
        
    },
    "components": {
        "securitySchemes": {
            "Authorization": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}
