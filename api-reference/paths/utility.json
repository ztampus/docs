{
    "openapi": "3.0.1",
    "info": {
        "title": "DigitalU API",
        "description": "DigitalU API",
        "license": {
            "name": "MIT"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://squadz.ngrok.app"
        }
    ],
    "security": [
        {
            "Authorization": []
        }
    ],

    "paths": {
        "/upload/": {
            "post": {
                "tags": ["Utility"],
                "summary": "Upload file",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "userid": {
                                        "type": "string",
                                        "example": "0A099B35713fb"
                                    },
                                    "uploadfile": {
                                        "type": "string",
                                        "format": "binary",
                                        "description": "File to upload"
                                    },
                                    "directory": {
                                        "type": "string"
                                    },
                                    "newfilename": {
                                        "type": "string",
                                        "description": "New name for the uploaded file"
                                    },
                                    "api": {
                                        "type": "string",
                                        "description": "API identifier"
                                    }
                                },
                                "required": ["uploadfile"]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "File uploaded successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string",
                                                "example": "P0J1gX0667425"
                                            },
                                            "key": {
                                                "type": "string",
                                                "example": "11/_video/heart/ages.mp4"
                                            },
                                            "url": {
                                                "type": "string",
                                                "example": "https://squadz.ngrok.app/download/11/_video/heart/ages.mp4"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
        }
    }
}
