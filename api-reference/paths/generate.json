    "paths": {
        "/generate": {
            "post": {
                "tags": ["Generate"],
                "summary": "Generate video",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": ["action", "videoid"],
                                "oneOf": [
                                    {
                                        "properties": {
                                            "action": { 
                                                "type": "string",
                                                "enum": ["create"]
                                            },
                                            "name": { "type": "string" },
                                            "text": { "type": "string" },
                                            "videoid": { "type": "string" }
                                        }
                                    },
                                    {
                                        "properties": {
                                            "action": { 
                                                "type": "string",
                                                "enum": ["update"]
                                            },
                                            "id": { "type": "string" },
                                            "name": { "type": "string" },
                                            "text": { "type": "string" },
                                            "videoid": { "type": "string" }
                                        },
                                        "required": ["id"]
                                    }
                                ],
                                "discriminator": {
                                    "propertyName": "action"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "result": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "createdate": {
                                                        "type": "string",
                                                        "format": "date-time"
                                                    },
                                                    "id": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
